<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>

    <properties resource="kanban/generatorConfig.properties"/>

    <!-- !!!! Driver Class Path !!!! -->
    <classPathEntry location="${driver.class.path}"/>

    <!--<context id="context" targetRuntime="MyBatis3Simple">-->
    <context id="context" targetRuntime="com.demo.boot.dao.codegen.DemoIntrospectedTableMyBatis3Impl">

        <property name="javaFileEncoding" value="UTF-8"/>

        <plugin type="com.demo.boot.dao.codegen.plugins.DemoJavaClientWrapperPlugin">
            <property name="javaClientTargetPackage" value="${mapper.package}"/>
            <property name="javaClientTargetProject" value="${mapper.project}"/>

            <property name="modelTargetPackage" value="${model.package}"/>
            <property name="modelTargetProject" value="${model.project}"/>
        </plugin>

        <!--为每个Dao生成一个扩展的sqlMap文件, 如UserMapperExt.xml-->
        <plugin type="com.demo.boot.dao.codegen.plugins.DemoMapperExtGeneratorPlugin"/>


        <!--默认注释生成器，后两项属性从1.3.2版本才有-->
        <!--<commentGenerator>-->
        <!--<property name="suppressAllComments" value="false"/>-->
        <!--<property name="suppressDate" value="false"/>-->
        <!--<property name="addRemarkComments" value="true"/>-->
        <!--<property name="dateFormat" value="yyyy-HH-mm hh:mm:ss"/>-->
        <!--</commentGenerator>-->

        <!--自定义注释生成器-->
        <commentGenerator type="com.demo.boot.dao.codegen.generator.DemoCommentGenerator">
            <property name="suppressAllComments" value="false"/>
        </commentGenerator>

        <!--jdbc:mysql://localhost:3306/demo-db?useUnicode=true&characterEncoding=utf-8-->
        <!-- !!!! Database Configurations !!!! -->
        <jdbcConnection driverClass="${jdbc.driver}"
                        connectionURL="${jdbc.url}"
                        userId="${jdbc.username}" password="${jdbc.password}"/>

        <javaTypeResolver type="com.demo.boot.dao.codegen.resolver.DemoJavaTypeResolver">
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!-- !!!! Model Configurations !!!! -->
        <javaModelGenerator targetPackage="${model.package}" targetProject="${model.project}">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="false"/>
        </javaModelGenerator>

        <!-- !!!! Mapper XML Configurations !!!! -->
        <sqlMapGenerator targetPackage="${xml.package}" targetProject="${xml.project}">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <!-- !!!! Mapper Interface Configurations !!!! -->
        <javaClientGenerator targetPackage="${mapper.package}" targetProject="${mapper.project}"
                             type="XMLMAPPER">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <!--!!!! Table Configurations !!!!-->
        <!--<table tableName="product" domainObjectName="Product" enableCountByExample="false" enableDeleteByExample="false"-->
        <!--enableSelectByExample="false"-->
        <!--enableUpdateByExample="false">-->

        <!--<columnOverride column="remark" javaType="java.lang.String" jdbcType="VARCHAR"/>-->
        <!--<columnOverride column="sizes" javaType="java.lang.String" jdbcType="VARCHAR"/>-->
        <!--<columnOverride column="propertys" javaType="java.lang.String" jdbcType="VARCHAR"/>-->
        <!--<columnOverride column="basketNum" javaType="java.lang.String" jdbcType="VARCHAR"/>-->
        <!--</table>-->
        <!--<table tableName="store_product_batch" domainObjectName="StoreProductBatch" enableCountByExample="false"-->
        <!--enableDeleteByExample="false"-->
        <!--enableSelectByExample="false"-->
        <!--enableUpdateByExample="false"/>-->

        <!--<table tableName="product_monitor" domainObjectName="ProductMonitor" enableCountByExample="false"-->
        <!--enableDeleteByExample="false"-->
        <!--enableSelectByExample="false"-->
        <!--enableUpdateByExample="false"/>-->
        <!--<table tableName="store_product_batch_history" domainObjectName="StoreProductBatchHistory"-->
        <!--enableCountByExample="false"-->
        <!--enableDeleteByExample="false"-->
        <!--enableSelectByExample="false"-->
        <!--enableUpdateByExample="false"/>-->
        <!--<table tableName="store_product_batch_monitor" domainObjectName="StoreProductBatchMonitor"-->
        <!--enableCountByExample="false"-->
        <!--enableDeleteByExample="false"-->
        <!--enableSelectByExample="false"-->
        <!--enableUpdateByExample="false"/>-->
